{
  "name": "dataaccessgateway",
  "version": "0.0.9",
  "description": "A simple library that cache in two different levels HTTP requests",
  "repository": {
    "type": "git",
    "url": "https://github.com/MrDesjardins/dataaccessgateway"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "webpack --env.production",
    "builddev": "node_modules/typescript/bin/tsc",
    "test": "node_modules/jest-cli/bin/jest.js --watch",
    "coverage": "node_modules/jest-cli/bin/jest.js --coverage"
  },
  "keywords": [
    "data",
    "access",
    "cache",
    "rest",
    "api",
    "http"
  ],
  "author": {
    "name": "Patrick Desjardins",
    "email": "mrdesjardins@gmail.com"
  },
  "license": "MIT",
  "dependencies": {
    "dexie": "^2.0.2"
  },
  "peerDependencies": {
    "axios": "^0.18.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/dexie": "^1.3.1",
    "@types/jest": "^22.2.3",
    "clean-webpack-plugin": "^0.1.19",
    "jest": "^22.4.3",
    "jest-cli": "^22.4.3",
    "ts-jest": "^22.4.4",
    "ts-loader": "^4.2.0",
    "typescript": "^2.8.3",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.15"
  },
  "jest": {
    "globals": {
      "__VERSION__": "test"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "transform": {
      "\\.tsx?$": "ts-jest"
    },
    "moduleNameMapper": {
      "^test/(.*)$": "<rootDir>/test/$1"
    },
    "testRegex": "\\.test\\.(js|ts)x?$"
  }
}